{
  "name": "@apps/contributor",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next | pino-pretty -c -t",
    "start": "next start",
    "build": "next build",
    "lint": "eslint --fix src/**/*.{ts,tsx}",
    "test": "run-p test:*",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "generate": "run-p generate:*",
    "generate:openapi": "run-s generate:openapi:*",
    "generate:openapi:trimmer": "openapi-endpoint-trimmer --url https://raw.githubusercontent.com/github/rest-api-description/main/descriptions/api.github.com/api.github.com.yaml --output github-api-spec.yaml --prefixes /search/repositories,/repos/{owner}/{repo}/readme,/repos/{owner}/{repo}/languages",
    "generate:openapi:validate": "swagger-cli validate github-api-spec.yaml",
    "generate:openapi:zod": "openapi-zod-client github-api-spec.yaml --output github-api-schemas.ts",
    "generate:prisma": "prisma generate",
    "ci:mock": "act --secret-file .env",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@mantine/core": "^6.0.10",
    "@mantine/dates": "^6.0.10",
    "@mantine/form": "^6.0.10",
    "@mantine/hooks": "^6.0.10",
    "@mantine/next": "^6.0.10",
    "@pinecone-database/pinecone": "^0.1.5",
    "@prisma/client": "^4.14.0",
    "@tabler/icons-react": "^2.18.0",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@vercel/analytics": "^1.0.1",
    "@vercel/postgres": "^0.3.0",
    "@zodios/core": "^10.9.0",
    "date-fns": "^2.30.0",
    "next": "13.4.2",
    "nextjs-cors": "^2.1.2",
    "octokit": "^2.0.14",
    "openai": "^3.2.1",
    "p-retry": "^5.1.2",
    "pino": "^8.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "superjson": "1.12.3",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@octokit/types": "^9.2.2",
    "@playwright/test": "^1.33.0",
    "@swc/core": "^1.3.57",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "eslint-define-config": "^1.20.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "openapi-endpoint-trimmer": "^2.0.0",
    "openapi-zod-client": "^1.6.4",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "typescript": "^5.0.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.3.2"
  }
}
